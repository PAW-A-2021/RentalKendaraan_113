@model IEnumerable<RentKendaraan.Models.Jaminan>

@{
    ViewData["Title"] = "Index";
}

<h1>Jaminan</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Jaminan")
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NamaJaminan)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdJaminan"><i class="fas fa-pencil-alt"></i></a> |
                <a asp-action="Details" asp-route-id="@item.IdJaminan"><i class="fas fa-info"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.IdJaminan"><i class="fas fa-trash-alt"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
